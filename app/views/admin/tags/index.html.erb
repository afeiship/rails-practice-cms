<h1>Admin Posts</h1>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Content</th>
    <th>Description</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @items.each do |item| %>
      <tr>
        <td><%= item.name %></td>
        <td><%= item.content %></td>
        <td><%= item.description %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

