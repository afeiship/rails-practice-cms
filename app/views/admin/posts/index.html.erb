<h1>Admin Posts</h1>

<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @admin_posts.each do |admin_post| %>
      <tr>
        <td><%= admin_post.title %></td>
        <td><%= admin_post.content %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

